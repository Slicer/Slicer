<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>
  Model Generation
  </category>
  <title>
  Probe Volume With Model (Paint)
  </title>
  <description>
  Paint a model by a volume (using vtkProbeFilter).
  </description>
  <version>0.1.0.$Revision: 1892 $(alpha)</version>
  <documentation-url></documentation-url>
  <license></license>
  <contributor>Lauren O'Donnell</contributor>
  <acknowledgements>BWH, NCIGT/LMI</acknowledgements>

  <parameters>
    <label>IO</label>
    <description>Input/output parameters</description>
    <image>
      <name>InputVolume</name>
      <label>Input Volume</label>
      <channel>input</channel>
      <index>0</index>
      <description>Volume to use to "paint" the model</description>
    </image>
    <geometry>
      <name>InputModel</name>
      <label>Input Model</label>
      <channel>input</channel>
      <index>1</index>
      <description>Input model</description>
    </geometry>
    <geometry>
      <name>OutputModel</name>
      <label>Output Model</label>
      <channel>output</channel>
      <index>2</index>
      <description>Output "painted" model</description>
    </geometry>
  </parameters>
  
</executable>
